{
  "name": "mongoose-tsgen",
  "description": "A Typescript interface generator for Mongoose that works out of the box.",
  "version": "9.5.3",
  "type": "module",
  "author": "Francesco Virga @francescov1",
  "bin": {
    "mtgen": "./bin/run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "bugs": "https://github.com/francescov1/mongoose-tsgen/issues",
  "dependencies": {
    "@oclif/core": "^4.5.4",
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "pluralize": "^8.0.0",
    "prettier": "^2.5.1",
    "resolve": "^1.22.8",
    "strip-json-comments": "^3.1.1",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^1.13.0"
  },
  "peerDependencies": {
    "mongoose": ">= 5.11.19",
    "typescript": ">= 4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint": "^7.2.4",
    "@types/flat": "^5.0.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.14.167",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^10.17.35",
    "@types/pluralize": "^0.0.33",
    "@types/prettier": "^2.7.3",
    "@types/resolve": "^1.20.6",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-oclif": "6.0.106",
    "eslint-config-oclif-typescript": "^3.1.14",
    "eslint-config-prettier": "^10.1.8",
    "globby": "^10.0.2",
    "husky": "^4.3.0",
    "jest": "^30.2.0",
    "lint-staged": "^10.5.1",
    "mongoose": "^8.18.2",
    "oclif": "^4.22.27",
    "rimraf": "^3.0.2",
    "rollup": "^4.52.3",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/francescov1/mongoose-tsgen",
  "keywords": [
    "mongoose",
    "generator",
    "typescript",
    "interfaces",
    "ts",
    "mtgen"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  },
  "oclif": {
    "bin": "mtgen"
  },
  "repository": "francescov1/mongoose-tsgen",
  "scripts": {
    "posttest": "eslint .",
    "build": "rollup -c",
    "prepack": "npm test && rm -rf lib && npm run build",
    "test": "npx jest --experimental-vm-modules",
    "readme": "./node_modules/@oclif/dev-cli/bin/run readme"
  },
  "types": "lib/index.d.ts",
  "lint-staged": {
    "*.{js,ts}": [
      "npx prettier --write",
      "eslint --cache --fix"
    ]
  }
}
